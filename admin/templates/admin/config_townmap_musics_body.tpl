<h1>{L_TOWNMAP_MUSICS_TITLE}</h1>

<p>{L_TOWNMAP_MUSICS_EXPLAIN}</p>

<form action="{S_CONFIG_ACTION}" method="post">

<table width="100%" cellpadding="4" cellspacing="1" border="0" class="forumline">
	<tr>
		<th class="thHead" colspan="2">{L_MUSIC}</th>
	</tr>
	<tr>
		<td align="center" class="row2" width="100%" colspan="2"><span class="gen">{L_MUSIC_DISABLE}</span></td>
	</tr>
	<tr>
		<td class="row1"><span class="gen">{L_SPRING}</span></td>
		<td class="row2" align="center"><input type="post" name="spring" maxlength="8" size="8" value="{SPRING}" /></td>
	</tr>
	<tr>
		<td class="row1"><span class="gen">{L_SUMMER}</span></td>
		<td class="row2" align="center"><input type="post" name="summer" maxlength="8" size="8" value="{SUMMER}" /></td>
	</tr>
	<tr>
		<td class="row1"><span class="gen">{L_AUTOMN}</span></td>
		<td class="row2" align="center"><input type="post" name="automn" maxlength="8" size="8" value="{AUTOMN}" /></td>
	</tr>
	<tr>
		<td class="row1"><span class="gen">{L_WINTER}</span></td>
		<td class="row2" align="center"><input type="post" name="winter" maxlength="8" size="8" value="{WINTER}" /></td>
	</tr>
</table>

<table width="100%" cellpadding="4" cellspacing="1" border="0" class="forumline">
	<tr>
		<td class="row2" colspan="2" align="center"><input type="submit" value="{L_SUBMIT}" name="music" class="liteoption" /></td>
	</tr>
</table>

<br clear="all" />

</form>
<br clear="all" />